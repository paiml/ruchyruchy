// test_wasm_simple.ruchy
// Simple test to verify basic WASM functionality

// Import the implementation
import bootstrap::stage3::wasmify_type
import ruchy::wasm::Type

fun main() {
    // Test primitive type mapping
    let i32_type = wasmify_type("i32")
    
    // Simple assert
    if (i32_type != Type.I32) {
        println("❌ FAILED: i32 should map to Type.I32")
    } else {
        println("✅ PASSED: i32 maps to Type.I32 correctly")
    }
}