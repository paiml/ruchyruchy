    return a / b;
}

// Power function
fun power(base: f64, exponent: i32) -> f64 {
    if exponent == 0 {
        return 1.0;
    }
    
    let mut result = 1.0;
    let mut abs_exp = if exponent < 0 { -exponent } else { exponent };
    
    for _ in 0..abs_exp {
        result *= base;
    }
    
    if exponent < 0 {
        return 1.0 / result;
    } else {
