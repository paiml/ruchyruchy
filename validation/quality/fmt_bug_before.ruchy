// Minimal reproduction for ruchy fmt vec! bug
// Bug: fmt changes valid vec![...] syntax to invalid macro vec(...) {}

fun main() {
    // Valid syntax: vec![item1, item2, item3]
    let items = vec![
        "item1",
        "item2",
        "item3",
    ]

    println("Items: " + items.len().to_string())
}

