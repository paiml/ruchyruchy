// Minimal reproduction for tuple return/destructuring
// Ruchy v3.94.0

fn return_tuple() -> (i32, i32) {
    (42, 100)
}

fn main() {
    let result = return_tuple();
    let first = result.0;
    let second = result.1;

    println("First: {}", first);
    println("Second: {}", second);
}

main();
