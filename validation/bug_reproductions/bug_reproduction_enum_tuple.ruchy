// Minimal reproduction for enum tuple variant pattern matching bug
// Ruchy v3.92.0

enum Position {
    Pos(i32, i32, i32)
}

fn get_line(pos: Position) -> i32 {
    match pos {
        Position::Pos(line, _, _) => line
    }
}

fn main() {
    let pos = Position::Pos(1, 2, 3);
    let line = get_line(pos);
    println("Line: {}", line);
}

main();
