}

enum Expr {
    Number(String),
    Binary(Op, Box<Expr>, Box<Expr>)
}

fun main() {
    let one = Expr::Number("1".to_string());
    let two = Expr::Number("2".to_string());
    let add = Expr::Binary(Op::Add, Box::new(one), Box::new(two));

