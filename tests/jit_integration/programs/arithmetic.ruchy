// JIT Integration Test: Arithmetic Operations
// Tests: +, -, *, /, %, comparison operators

fun main() {
    // Basic arithmetic
    let a = 10 + 5;
    let b = 20 - 3;
    let c = 6 * 7;
    let d = 100 / 4;
    let e = 17 % 5;

    // Combined operations
    let result = a + b + c + d + e;

    return result;  // Expected: 15 + 17 + 42 + 25 + 2 = 101
}
