// JIT Integration Test: Control Flow
// Tests: if/else, while loops, for loops

fun main() {
    let sum = 0;

    // If expression
    let x = 10;
    if x > 5 {
        sum = sum + 1;
    }

    // While loop
    let i = 0;
    while i < 5 {
        sum = sum + i;
        i = i + 1;
    }

    // For loop with range
    for j in 0..3 {
        sum = sum + j;
    }

    return sum;  // Expected: 1 + (0+1+2+3+4) + (0+1+2) = 1 + 10 + 3 = 14
}
