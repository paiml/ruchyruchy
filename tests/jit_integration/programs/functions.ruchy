// JIT Integration Test: Functions
// Tests: function calls, parameters, return values

fun add(a, b) {
    return a + b;
}

fun multiply(x, y) {
    return x * y;
}

fun factorial(n) {
    if n <= 1 {
        return 1;
    }
    return n * factorial(n - 1);
}

fun main() {
    let a = add(10, 20);
    let b = multiply(5, 6);
    let c = factorial(5);

    return a + b + c;  // Expected: 30 + 30 + 120 = 180
}
