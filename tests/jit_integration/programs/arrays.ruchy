// JIT Integration Test: Arrays
// Tests: array literals, indexing, vec![] macro

fun main() {
    // Array literal
    let arr = [10, 20, 30, 40];
    let sum = arr[0] + arr[1] + arr[2] + arr[3];

    // vec![] macro - list form
    let v1 = vec![5, 10, 15];
    sum = sum + v1[0] + v1[1] + v1[2];

    // vec![] macro - repeat form
    let v2 = vec![7; 3];
    sum = sum + v2[0] + v2[1] + v2[2];

    return sum;  // Expected: 100 + 30 + 21 = 151
}
