// Conference and Workshop Kit for RuchyRuchy
// Comprehensive toolkit for organizing educational events

// Workshop type
enum WorkshopType {
    Introduction,
    Hands_On,
    Advanced,
    Masterclass,
    Lightning
}

// Event format
enum EventFormat {
    In_Person,
    Virtual,
    Hybrid,
    Self_Paced
}

// Workshop session
struct WorkshopSession {
    title: str,
    duration_minutes: i32,
    workshop_type: WorkshopType,
    max_participants: i32,
    prerequisites: str,
    materials_needed: str
}

// Workshop agenda
struct WorkshopAgenda {
    session_count: i32,
    total_duration_minutes: i32,
    break_count: i32,
    format: EventFormat
}

// Presenter guide
struct PresenterGuide {
    experience_level: str,
    preparation_time_hours: i32,
    technical_requirements: str,
    backup_plans: str
}

// Participant materials
struct ParticipantMaterials {
    setup_guide: str,
    code_samples: str,
    exercises: str,
    reference_sheets: str,
    certificate: bool
}

// Create workshop session
fn create_workshop_session(title: str, duration: i32, workshop_type: WorkshopType) -> WorkshopSession {
    WorkshopSession {
        title: title,
        duration_minutes: duration,
        workshop_type: workshop_type,
        max_participants: 30,
        prerequisites: "Basic programming knowledge",
        materials_needed: "Laptop with internet access"
    }
}

// Create workshop agenda
fn create_agenda(format: EventFormat) -> WorkshopAgenda {
    WorkshopAgenda {
        session_count: 4,
        total_duration_minutes: 240,
        break_count: 3,
        format: format
    }
}

