// Test enum tuple variant inside a tuple return value
// Ruchy v3.94.0

enum Token {
    Tok(i32, String)
}

fn make_token() -> (Token, i32) {
    let tok = Token::Tok(42, "hello".to_string());
    (tok, 100)
}

fn main() {
    let result = make_token();
    let token = result.0;
    let idx = result.1;

    match token {
        Token::Tok(num, text) => {
            println("Token: Tok({}, \"{}\")", num, text);
            println("Index: {}", idx);
        }
    }
}

main();
