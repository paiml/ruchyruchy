// Verify Box<T> works in v3.96.0
enum Tree {
    Leaf(i32),
    Node(Box<Tree>, Box<Tree>)
}

fun main() {
    println("Testing Box<T> in enum variants (v3.96.0)...");

    let left = Tree::Leaf(1);
    let right = Tree::Leaf(2);
    let tree = Tree::Node(Box::new(left), Box::new(right));

    match tree {
        Tree::Node(_, _) => println("âœ… Box<Tree> in enum works!"),
        Tree::Leaf(_) => println("Leaf")
    }
}

main();
