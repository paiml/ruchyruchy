fun multiplication_table(size: i32) {
    let mut i = 1;
    while i <= size {
        let mut j = 1;
        while j <= size {
            let product = i * j;
            println("{} x {} = {}", i, j, product);
            j = j + 1;
        }
        i = i + 1;
    }
}

fun main() {
    multiplication_table(3);
}
