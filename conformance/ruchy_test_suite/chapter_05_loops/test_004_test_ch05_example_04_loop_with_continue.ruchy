fun print_odd_numbers(n: i32) {
    let mut i = 1;
    while i <= n {
        if i % 2 == 0 {
            i = i + 1;
            continue;
        }
        println("Odd: {}", i);
        i = i + 1;
    }
}

fun main() {
    print_odd_numbers(10);
}
